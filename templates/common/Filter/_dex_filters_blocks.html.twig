{% block anyTypes_row %}
	{% block anyTypes_label %}
		<label for="any_type" class="form-label">
			<i class="bi bi-subtract"></i>
			{{ 'dex.filters.any_type.label'|trans }}
		</label>
	{% endblock anyTypes_label %}
	{% block anyTypes_widget %}
    {% set filteredAnyTypes = app.request.get('at', [])|filter(v => v) %}
		<select multiple id="any_type" name="at[]" class="form-select form-select-sm">
			<option value="" {{ ((filteredAnyTypes|filter(v => v) is empty) ? 'selected': '') }}>
				{{- 'dex.filters.any_type.value.empty'|trans -}}
			</option>
			{% for item in types %}
				<option value="{{ item.slug }}" {{ ((item.slug in filteredAnyTypes) ? 'selected': '') }}>
					{{- (locale is same as('fr') ? item.frenchName : item.name) -}}
				</option>
			{% endfor %}
		</select>
	{% endblock anyTypes_widget %}
{% endblock anyTypes_row %}

